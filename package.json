{
  "name": "@hoverbaum/slides-to-pdf",
  "version": "0.4.0",
  "description": "A simple utility to export HTML based presentation to PDF.",
  "main": "lib/slides-to-pdf.js",
  "bin": "bin/index.js",
  "scripts": {
    "standard": "standard \"lib/**/*.js\" \"bin/**/*.js\"",
    "test:dependencies:allareused": "dependency-check --unused --no-dev package.json",
    "test:dependencies:allarelisted": "dependency-check package.json",
    "test": "npm run standard && npm run test:dependencies:allareused && npm run test:dependencies:allarelisted"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HoverBaum/slides-to-pdf.git"
  },
  "author": "Hendrik Wallbaum <mail@hendrikwallbaum.de>",
  "dependencies": {
    "commander": "^2.20.0",
    "easy-pdf-merge": "^0.2.1",
    "fs-extra": "^7.0.1",
    "listr": "^0.14.3",
    "puppeteer": "^1.14.0"
  },
  "devDependencies": {
    "dependency-check": "^3.3.0",
    "standard": "^12.0.1"
  }
}
